<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="gov.nih.nci.security.dao.hibernate.RolePrivilege" 
    table="role_privilege"
>

    <id
        name="rolePrivilegeId"
        type="java.lang.Long"
        column="ROLE_PRIVILEGE_ID"
        unsaved-value="null"
    >
        <generator class="identity"/>
    </id>

    <property
        name="updateDate"
        type="java.sql.Date"
        column="UPDATE_DATE"
        not-null="true"
        length="10"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Privilege -->
    <many-to-one
        name="privilege"
        class="gov.nih.nci.security.authorization.domainobjects.Privilege"
        not-null="true"
    >
        <column name="PRIVILEGE_ID" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Role -->
    <many-to-one
        name="role"
        class="gov.nih.nci.security.authorization.domainobjects.Role"
        not-null="true"
    >
        <column name="ROLE_ID" />
    </many-to-one>

</class>
</hibernate-mapping>
